#include<stdio.h>
//#include<conio.h>
#include<string.h>
int main()
{
char a[10000000];
long int l,i,count=1;
scanf("%s",a);
l=strlen(a);


   for(i=0;i<l;i++)
   {
        if(a[i]>='A' &&a[i]<='Z')
        {
                     count=count+1;
        }
   }
   printf("%ld",count);
   // getch();
    return 0;

}